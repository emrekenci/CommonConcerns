<?xml version="1.0"?>
<package>
  <metadata>
    <id>CommonConcerns</id>
    <version>$version$</version>
    <authors>Emre Kenci</authors>
    <owners>Emre Kenci</owners>
    <description>
      CommonConcerns allows to rapidly build software by less boilerplate code like logging, exception handling, sending an email
    </description>
    <copyright>Copyright Emre Kenci</copyright>
    <language>en-US</language>
    <dependencies>
      <dependency id="Newtonsoft.Json" version="8.0.3"/>
      <dependency id="PostSharp" version="4.2.22"/>
      <dependency id="Microsoft.AspNet.WebApi.Client" version="5.2.3" />
    </dependencies>
  </metadata>
  <files>
    <file src="bin\$configuration$\Common.dll" target="lib\net45" />
    <file src="bin\$configuration$\Common.pdb" target="lib\net45" />
    <file src="bin\$configuration$\Common.dll" target="lib\net451" />
    <file src="bin\$configuration$\Common.pdb" target="lib\net451" />
    <file src="app.config.install.xdt" target="content" />
    <file src="web.config.install.xdt" target="content" />
    <file src="app.config.uninstall.xdt" target="content" />
    <file src="web.config.uninstall.xdt" target="content" />
    <file src="**\*.cs" target="src" />
  </files>
</package>